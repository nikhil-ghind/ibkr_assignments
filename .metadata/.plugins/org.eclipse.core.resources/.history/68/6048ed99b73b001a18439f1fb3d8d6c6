package def;

public class PersonWithHashCode {
	public String name;
	public String email;
	public String phoneNumber;
	public int id;
	
	PersonWithHashCode(String name,String email,String phoneNumbe,int id){
		this.name = name;
		this.email = email;
		this.phoneNumber = phoneNumber;
		this.id = id;
	}
	PersonWithHashCode(){
		this.name = "Nikhil";
		this.email = "ghind20@gmail.com";
		this.phoneNumber = "7021197094";
	}
	
	@Override
	public int hashCode() {
		return id;
	}
	
	@Override
	public String toString() {
		return this.name + this.email + this.phoneNumber;
	}
}
